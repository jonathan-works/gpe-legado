<?xml version="1.0" encoding="UTF-8"?>

<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

	<begin-conversation join="true"/>
	<restrict>#{security.checkPage()}</restrict>

	<param name="order" value="#{parametroList.order}" />
	<param name="restrictionLogicOperator" value="#{parametroList.restrictionLogicOperator}" />
	<param name="ativo" value="#{parametroList.entity.ativo}" />
	<param name="nomeVariavel" value="#{parametroList.entity.nomeVariavel}" />
	<param name="descricaoVariavel" value="#{parametroList.entity.descricaoVariavel}" />
	<param name="valorVariavel" value="#{parametroList.entity.valorVariavel}" />
	<param name="dataAtualizacao" value="#{parametroList.entity.dataAtualizacao}" />
	<param name="sistema" value="#{parametroList.entity.sistema}" />
	<param name="usuarioModificacao" value="#{parametroList.entity.usuarioModificacao}" />
	
</page>