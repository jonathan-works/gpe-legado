<?xml version="1.0" encoding="UTF-8"?>

<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

	<restrict>#{security.checkPage()}</restrict>

	<param name="order" value="#{tipoComunicacaoList.order}" />
	<param name="restrictionLogicOperator" value="#{tipoComunicacaoList.restrictionLogicOperator}" />
	<param name="ativo" value="#{tipoComunicacaoList.entity.ativo}" />
	<param name="quantidadeDiasCiencia" value="#{tipoComunicacaoList.entity.quantidadeDiasCiencia}" converterId="integerConverter"/>
	<param name="descricao" value="#{tipoComunicacaoList.entity.descricao}" />
	
</page>